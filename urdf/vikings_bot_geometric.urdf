<?xml version="1.0"?>
<robot name="urdfbot">
        
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </visual>
  </link>

  <link name="head_link">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </visual>
  </link>

    <joint name="base_link_to_head_link_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0.11" />
        <parent link="base_link" />
        <child link="head_link" />
    </joint>

</robot>
